{
  "name": "nestjs-api-tutorial",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:prod:deploy": "NEST_ENV='production' prisma migrate deploy",
    "prisma:dev:deploy": "NEST_ENV='development' prisma migrate deploy",
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && sleep 1 && yarn prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -e '.env.test' -- prisma migrate deploy",
    "db:test:rm": "docker compose -f docker-compose-dev.yml rm test-db -s -f -v",
    "db:test:up": "docker compose -f docker-compose-dev.yml up test-db -d",
    "db:test:restart": "dotenv -e '.env.test' yarn db:test:rm && yarn db:test:up && sleep 1 && yarn prisma:test:deploy",
    "prebuild": "rimraf dist",
    "build": "nest build && yarn build:views",
    "build:views": "yarn delete:views && yarn copy:views",
    "delete:views": "rm -rf ./dist/views",
    "copy:views": "cp -r ./views ./dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NEST_ENV='development' nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NEST_ENV='production' node dist/main",
    "docker:start:dev": "docker compose -f docker-compose-dev.yml up",
    "docker:start:prod": "docker compose -f docker-compose.yml up",
    "docker:stop:dev": "docker compose -f docker-compose-dev.yml down",
    "docker:stop:prod": "docker compose -f docker-compose.yml down",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "yarn db:test:restart",
    "test:e2e": "dotenv -e '.env .test' -- jest --watch --no-cache --config ./test/jest-e2e.json",
    "test:unit": "dotenv -e '.env .test' -- jest --watch --detectOpenHandles --no-cache --config ./test/jest-unit.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^10.3.3",
    "@prisma/client": "5",
    "@types/express-handlebars": "^6.0.0",
    "@types/nodemailer": "^6.4.14",
    "argon2": "^0.31.2",
    "bull": "^4.12.2",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-store": "2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^3.3.1",
    "ejs": "^3.1.9",
    "express-handlebars": "^5.3.0",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/testing": "^10.3.3",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/chai": "^4.3.12",
    "@types/express": "^4.17.21",
    "@types/hbs": "^4.0.4",
    "@types/jest": "27.0.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.27",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^5.1.0",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "mocha": "^10.3.0",
    "pactum": "^3.1.3",
    "prettier": "^2.3.2",
    "prisma": "5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.10.1",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest-setup.ts"
    ]
  }
}
